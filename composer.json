{
    "name": "casperhr/laravel-fractal",
    "description": "An easy to use Fractal integration for Laravel applications",
    "keywords": [
        "spatie",
        "laravel-fractal",
        "laravel",
        "fractal",
        "lumen",
        "api",
        "transform"
    ],
    "homepage": "https://github.com/spatie/laravel-fractal",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
     "repositories": [
        {
            "type":"package",
            "package": {
                "name": "casperhr/fractalistic",
                "version":"master",
                "source": {
                    "url": "https://github.com/casperhr/fractalistic.git",
                    "type": "git",
                    "reference":"main"
                }
            }
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0",
        "casperhr/fractalistic": "dev-master",
        "spatie/laravel-package-tools": "^1.11"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Fractal\\": "src"
        },
        "files": [
            "src/helpers.php"

        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Fractal\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Fractal\\FractalServiceProvider"
            ],
            "aliases": {
                "Fractal": "Spatie\\Fractal\\Facades\\Fractal"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
